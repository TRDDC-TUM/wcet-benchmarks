      <eventDataPort name="RC_Evt_Input"/>
      <dataPort name="RC_Data_Output" direction="out" dataClassifier="/aadlSpec[@name=Data_Types]/packageSpec[@name=Data_Types]/aadlPublic/dataImpl[@name=Tableau.Generic]"/>
